<?xml version="1.0" encoding="UTF-8"?>
<!-- 
  Entry list for a relay event, containing teams, team lineups and the classes entered.
-->
<EntryList xmlns="http://www.orienteering.org/datastandard/3.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           iofVersion="3.0"
           createTime="2011-07-20T12:16:31+02:00"
           creator="Example Software">
  <Event>
    <Name>Example event</Name>
    <StartTime>
      <Date>2011-07-31</Date>
      <Time>10:00:00+01:00</Time>
    </StartTime>
  </Event>
  <TeamEntry>
    <!-- the name of the team, usually including an ordinal number if the club has many teams entered to the same class -->
    <Name>Forest Vagabonds 1</Name>
    <!-- if the team consists of competitors representing multiple clubs, use one Organisation element for each club -->
    <Organisation>
      <Id>4</Id>
      <Name>Forest Vagabonds</Name>
      <Country code="GB">Great Britain</Country>
    </Organisation>
    <!-- the TeamEntryPerson elements hold the team lineup information, and should be omitted if the lineup is not known -->
    <TeamEntryPerson>
      <Person>
        <Id>3</Id>
        <Name>
          <Family>Reid</Family>
          <Given>Samuel</Given>
        </Name>
      </Person>
      <Leg>1</Leg>
      <!-- the ControlCard element is to be omitted if the competitor wishes to rent a control card -->
      <ControlCard>54321</ControlCard>
    </TeamEntryPerson>
    <TeamEntryPerson>
      <Person>
        <Id>4</Id>
        <Name>
          <Family>Fisher</Family>
          <Given>Penny</Given>
        </Name>
      </Person>
      <Leg>2</Leg>
      <!-- LegOrder is used only when the relay contains parallel legs or when a particular leg allows multiple competitors -->
      <LegOrder>1</LegOrder>
      <ControlCard>59476</ControlCard>
    </TeamEntryPerson>
    <TeamEntryPerson>
      <Person>
        <Id>5</Id>
        <Name>
          <Family>Collins</Family>
          <Given>Anne</Given>
        </Name>
      </Person>
      <Leg>2</Leg>
      <LegOrder>2</LegOrder>
      <ControlCard>96463</ControlCard>
    </TeamEntryPerson>
    <TeamEntryPerson>
      <Person>
        <Id>6</Id>
        <Name>
          <Family>Hall</Family>
          <Given>Brent</Given>
        </Name>
      </Person>
      <Leg>2</Leg>
      <LegOrder>3</LegOrder>
      <ControlCard>10583</ControlCard>
    </TeamEntryPerson>
    <TeamEntryPerson>
      <Person>
        <Id>7</Id>
        <Name>
          <Family>Jenkins</Family>
          <Given>Allison</Given>
        </Name>
      </Person>
      <Leg>3</Leg>
      <ControlCard>68490</ControlCard>
    </TeamEntryPerson>
    <Class>
      <Id>1</Id>
      <Name>Open</Name>
    </Class>
    <EntryTime>2011-07-12T21:05:58+01:00</EntryTime>
  </TeamEntry>
  <!-- an example of a team where the team members represent different clubs, known as a combination team -->
  <TeamEntry>
    <!-- all clubs making up the combination team are normally present in the team's name -->
    <Name>Super Fast Orienteers / Unintentional Navigators</Name>
    <!-- one Organisation element for each club -->
    <Organisation>
      <Id>1</Id>
      <Name>Super Fast Orienteers</Name>
      <Country code="GB">Great Britain</Country>
    </Organisation>
    <Organisation>
      <Id>3</Id>
      <Name>Unintentional Navigators</Name>
      <Country code="GB">Great Britain</Country>
    </Organisation>
    <TeamEntryPerson>
      <Person>
        <Id>8</Id>
        <Name>
          <Family>Parker</Family>
          <Given>Jeffrey</Given>
        </Name>
      </Person>
      <!-- if a combination team member's club membership is known, it may be expressed by using the Organisation element -->
      <Organisation>
        <Id>1</Id>
        <Name>Super Fast Orienteers</Name>
        <Country code="GB">Great Britain</Country>
      </Organisation>
      <Leg>2</Leg>
      <ControlCard>33354</ControlCard>
