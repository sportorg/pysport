            <Position lat="59.763947" lng="17.685403"/>
            <MapPosition unit="mm" x="252" y="118"/>
          </Control>
          <MapText>1</MapText>
          <MapTextPosition unit="mm" x="-15" y="8"/>
          <LegLength>410</LegLength>
        </CourseControl>
        <CourseControl type="Control">
          <Control>
            <Id>34</Id>
            <Position lat="59.75820" lng="17.685711"/>
            <MapPosition unit="mm" x="154" y="49"/>
          </Control>
          <MapText>2</MapText>
          <MapTextPosition unit="mm" x="11" y="-14"/>
          <LegLength>340</LegLength>
        </CourseControl>
        <CourseControl type="Control">
          <Control>
            <Id>100</Id>
            <Position lat="59.761130" lng="17.684938"/>
            <MapPosition unit="mm" x="173" y="121"/>
          </Control>
          <MapText>3</MapText>
          <MapTextPosition unit="mm" x="-12" y="12"/>
          <LegLength>800</LegLength>
        </CourseControl>
        <CourseControl type="Finish" >
          <Control>
            <Id>F</Id>
            <Position lat="59.75991" lng="17.68820"/>
            <MapPosition unit="mm" x="131" y="157"/>
          </Control>
          <LegLength>150</LegLength>
        </CourseControl>
      </Course>
    </CourseGroup>
    
    <!-- the class(es) using the course(s) in the course group -->
    <Class>
      <Id>1</Id>
      <Name>Men Open</Name>
    </Class>
    <Class>
      <Id>2</Id>
      <Name>Women Open</Name>
    </Class>
  </CourseClassData>

  <!-- assigning courses to a relay team -->
  <TeamCourseCombination>
    <!-- the identifier of the team, known by both systems exchanging data -->
    <TeamId>1</TeamId>
    <!-- the course assigned to the competitior running the first leg for the team -->
    <CourseAssignment>
      <BibNumber>101</BibNumber>
      <Leg>1</Leg>
      <Course>
        <Id>1</Id>
        <Name>Open A</Name>
      </Course>
    </CourseAssignment>
    <!-- the course assigned to the competitior running the second leg for the team -->
    <CourseAssignment>
      <BibNumber>101</BibNumber>
      <Leg>1</Leg>
      <Course>
        <Id>2</Id>
        <Name>Open B</Name>
      </Course>
    </CourseAssignment>
    <!-- the course assigned to the competitior running the third leg for the team -->
    <CourseAssignment>
      <BibNumber>101</BibNumber>
      <Leg>1</Leg>
      <Course>
        <Id>3</Id>
        <Name>Open C</Name>
      </Course>
    </CourseAssignment>
  </TeamCourseCombination>

  <!-- assigning courses to another relay team -->
  <TeamCourseCombination>
    <TeamId>2</TeamId>
    <CourseAssignment>
      <BibNumber>102</BibNumber>
      <Leg>1</Leg>
      <Course>
        <Id>3</Id>
